<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - carlos-feli-portfolio</title>


    <environment include="Development">
        <script  defer src="@Url.Content("~/js/jquery-3.3.1.slim.min.js")" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
        <script  defer src="@Url.Content("~/js/popper.min.js")" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
        <script  defer src="@Url.Content("~/js/bootstrap.min.js")" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
        <link rel="stylesheet" href="@Url.Content("~/css/bootstrap.min.css")" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="@Url.Content("~/css/site.css")" />
    </environment>
    <environment exclude="Development">
        <script defer src="@Url.Content("~/js/jquery-3.3.1.slim.min.js")" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
        <script defer src="@Url.Content("~/js/popper.min.js")" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
        <script defer src="@Url.Content("~/js/bootstrap.min.js")" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
        <link rel="stylesheet" href="@Url.Content("~/css/bootstrap.min.css")" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="@Url.Content("~/css/site.css")" />
    </environment>

</head>
<body style="padding-top:unset; padding-bottom:unset">
    <div class="container body-content">
        @RenderBody()
    </div>

    <environment include="Development">
        <script defer src="@Url.Content("~/js/jquery-3.3.1.slim.min.js")"></script>
        <script defer src="@Url.Content("~/js/bootstrap.min.js")"></script>
        <script defer src="@Url.Content("~/js/bootstrap.min.js")" asp-append-version="true"></script>
        <link rel="stylesheet" href="@Url.Content("~/css/bootstrap.min.css")" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="@Url.Content("~/css/site.css")" />
    </environment>
    <environment exclude="Development">
        <script defer src="@Url.Content("~/js/jquery-3.3.1.slim.min.js")"
                asp-fallback-src="@Url.Content("~/js/jquery-3.3.1.slim.min.js")"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
        </script>
        <script defer src="@Url.Content("~/js/bootstrap.min.js")"
                asp-fallback-src="@Url.Content("~/js/bootstrap.min.js")"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script defer src="@Url.Content("~/js/bootstrap.min.js")" asp-append-version="true"></script>
        <link rel="stylesheet" href="@Url.Content("~/css/bootstrap.min.css")" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="@Url.Content("~/css/site.css")" />
    </environment>




    @RenderSection("Scripts", required: false)
</body>
</html>
